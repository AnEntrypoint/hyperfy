{
  "name": "hyperfy-sdk",
  "version": "1.0.1",
  "description": "Official Hyperfy SDK for Node.js - Build immersive 3D worlds and virtual reality experiences",
  "main": "./src/index.js",
  "types": "./src/index.ts",
  "type": "module",
  "files": "[\"src/\"]",
  "scripts": {
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "hyperfy",
    "vr",
    "virtual-reality",
    "3d",
    "metaverse",
    "sdk",
    "nodejs",
    "websocket",
    "realtime",
    "gaming",
    "immersive"
  ],
  "author": "Hyperfy Team <team@hyperfy.com>",
  "license": "MIT",
  "homepage": "https://github.com/AnEntrypoint/hyperfy-sdk#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/AnEntrypoint/hyperfy-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/AnEntrypoint/hyperfy-sdk/issues"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "eventemitter3": "^5.0.1",
    "form-data": "^4.0.0",
    "formidable": "^3.5.1",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.10",
    "msgpackr": "^1.9.9",
    "node-fetch": "^3.3.2",
    "three": "^0.160.0",
    "uuid": "^9.0.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/commander": "^2.12.2",
    "@types/dotenv": "^8.1.0",
    "@types/formidable": "^3.4.3",
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.7",
    "@types/ws": "^8.5.8",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "rollup": "^4.3.0",
    "rollup-plugin-dts": "^6.1.0",
    "@rollup/plugin-terser": "^0.4.4",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "typedoc": "^0.25.2",
    "typescript": "^5.2.2",
    "ws": "^8.14.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "hyperfy-sdk": "./dist/cli.js"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src",
      "<rootDir>/tests"
    ],
    "testMatch": [
      "**/__tests__/**/*.ts",
      "**/?(*.)+(spec|test).ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/types/**/*"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    },
    "testTimeout": 30000
  }
}
